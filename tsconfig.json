{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "lib": ["DOM","ESNext"],
    "outDir": "./dist",
    "rootDir": "./src",
    "removeComments": true,
    "noEmitOnError": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },

  "include": [
    "./src"
  ]
}